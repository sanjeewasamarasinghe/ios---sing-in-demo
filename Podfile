  # Pods for MyLyrics

	pod 'Alamofire'
	pod 'SwiftyJSON'
	pod 'Firebase/Core'
	pod 'Firebase/Auth'
	pod 'GoogleSignIn'
	pod 'Firebase/Database'

  target 'MyLyricsTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MyLyricsUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
